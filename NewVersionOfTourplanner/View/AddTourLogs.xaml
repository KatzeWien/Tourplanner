<Window x:Class="NewVersionOfTourplanner.View.AddTourLogs"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NewVersionOfTourplanner.View"
        mc:Ignorable="d"
        Title="AddTourLogs" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock Text="Date"/>
            <TextBox x:Name="inputDate"/>
            <TextBlock Text="Comment"/>
            <TextBox x:Name="Comment"/>
            <TextBlock Text="Difficulty"/>
            <TextBox x:Name="Difficulty"/>
            <TextBlock Text="Total Distance"/>
            <TextBox x:Name="TotalDistance"/>
            <TextBlock Text="Total Time"/>
            <TextBox x:Name="TotalTime"/>
            <TextBlock Text="Rating"/>
            <TextBox x:Name="Rating"/>
            <Button Click="addValuesBtn_Click">Submit</Button>
        </StackPanel>
    </Grid>
</Window>
